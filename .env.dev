# Kafka
## kafka1
KAFKA1_NODE_ID=1
KAFKA1_HOST_NAME=kafka-1
KAFKA1_HOST_PORT=19092
KAFKA1_CONTAINER_PORT=9092
KAFKA1_PROCESS_ROLES=broker,controller

KAFKA1_LISTENERS=PLAINTEXT://kafka-1:9092,CONTROLLER://kafka-1:9093,INTERNAL://kafka-1:9094
KAFKA1_ADVERTISED_LISTENERS=PLAINTEXT://kafka-1:9092
KAFKA1_LISTENER_SECURITY_PROTOCOL_MAP=PLAINTEXT:PLAINTEXT,CONTROLLER:PLAINTEXT,INTERNAL:PLAINTEXT

KAFKA1_CONTROLLER_LISTENER_NAMES=CONTROLLER
KAFKA1_CONTROLLER_QUORUM_VOTERS=1@kafka-1:9093,2@kafka-2:9093,3@kafka-3:9093

KAFKA1_OFFSETS_TOPIC_REPLICATION_FACTOR=3 #consumer_offset topic replication
KAFKA1_TRANSACTION_STATE_LOG_REPLICATION_FACTOR=3 #transaction_state topic replication
KAFKA1_TRANSACTION_STATE_LOG_MIN_ISR=2
KAFKA1_GROUP_INIT_REBALANCE_DELAY_MS=0
KAFKA1_NUM_PARTITIONS=3

